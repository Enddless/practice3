{
  "encoding": "UTF-8",
  "headers": {
    "/*": {
      "Access-Control-Allow-Origin": "*",
      "Content-Type": "text/html; charset=UTF-8"
    },
    "/sample*": {
      "Access-Control-Allow-Origin": "*",
      "Content-Type": "text/plain; charset=UTF-8"
    },
    "/fetch/*": {
      "Access-Control-Allow-Origin": "no-cors",
      "Content-Type": "text/plain; charset=UTF-8"
    },
    "/login/*": {
      "Access-Control-Allow-Origin": "*",
      "Content-Type": "text/plain; charset=UTF-8"
    }
  }    
}
